+  virtual void Execute() = 0;
+
+  uv_work_t request;
+
+ protected:
+  v8::Persistent<v8::Object> persistentHandle;
+  NanCallback *callback;
+  const char *errmsg;
+
+  virtual void HandleOKCallback() {
+    NanScope();
+
+    callback->Call(0, NULL);
