+  var that = this;
+  process.nextTick(function () {
+    if (that.bytesAhead() === 0) {
+      that.destroy();
+    }
+  });
+};
+
+Reader.prototype._moveOffset = function(relativeOffset) {
+  this._setOffset(this._offset + relativeOffset);
+  return this._offset - relativeOffset;
+};
