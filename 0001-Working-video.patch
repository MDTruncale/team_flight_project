+    NanAssignPersistent(persistentHandle, obj);
+  }
+
+  virtual ~NanAsyncWorker() {
+    NanScope();
+
+    if (!persistentHandle.IsEmpty())
+      NanDisposePersistent(persistentHandle);
+    if (callback)
+      delete callback;
+    if (errmsg)
+      delete errmsg;
+  }
+
+  virtual void WorkComplete() {
+    NanScope();
+
+    if (errmsg == NULL)
+      HandleOKCallback();
