+// * Consistent prefixes/suffixes are stripped
+// * Always converted to UPPER_CASE
+
+var constants = module.exports;
+
+constants.getName = function(group, value) {
+  group = this[group];
+
+  for (var name in group) {
+    if (group[name] === value) {
+      return name;
+    }
+  }
